{
  "name": "docker-ko.github.io",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier:check": "prettier --check \"{src,tests}/**/*.{js,jsx,ts,tsx,json,css,scss}\"",
    "prettier:write": "prettier --write \"{src,tests}/**/*.{js,jsx,ts,tsx,json,css,scss}\"",
    "type:check": "tsc",
    "fix:all": "npm run lint:fix && npm run prettier:write",
    "check:all": "npm run lint:check && npm run prettier:check && npm run type:check",
    "test": "vitest --run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@tailwindcss/vite": "^4.1.7",
    "@types/jsdom": "^21.1.7",
    "eslint": "^9.27.0",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.33.0",
    "vite": "^6.3.2",
    "vitest": "^3.2.2"
  },
  "dependencies": {
    "@microsoft/applicationinsights-web": "^3.3.9",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-vue": "^5.2.1",
    "marked": "^15.0.7",
    "tailwindcss": "^4.0.6",
    "vite-plugin-markdown": "^2.2.0"
  }
}
